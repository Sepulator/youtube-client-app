{
  "spellright.language": ["en"],
  "spellright.documentTypes": ["latex", "plaintext", "markdown"],
  "eslint.validate": ["javascript", "typescript", "html"],

  "eslint.options": {
    "extensions": [".js", ".ts", "html"]
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
